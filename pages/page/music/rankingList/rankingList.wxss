/* pages/page/music/rankingList/rankingList.wxss */
.ranking_list .li{
    margin: 25rpx;
    padding: 25rpx;
    border-bottom-right-radius: 100rpx;
    border-top-left-radius: 100rpx;
    overflow: hidden;
    /* background-color: #3dcc9c; */
    box-shadow: 0 0rpx 10rpx rgba(0,0,0,.1);
}
.ranking_list .images{
    width: 200rpx;
    min-width: 200rpx;
    height: 200rpx;
    border-radius: 50%;
    margin-right: 25rpx;
    overflow: hidden;
}
.ranking_list .images image{
    width: 100%;
}
.ranking_list .message{
    font-size: 30rpx;
    color: #333;
    overflow: hidden;
    text-overflow:ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 3;
    -webkit-box-orient: vertical;
}