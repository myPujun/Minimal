@import '../../../app';
.suspension{
    position: fixed;
    bottom: 100rpx;
    right: 50rpx;
    width: 100rpx;
    height: 100rpx;
    border-radius: 50%;
    overflow: hidden;
    z-index: 9999;
}
.suspension image{
    width: 100%;
    height: 100%;
}
.suspension .icon-zanting,.suspension .icon-bofang{
    display: flex;
    align-items: center;
    justify-content: center;
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    font-size: 80rpx;
    color:#3dcc9c;
    background-color: rgba(0,0,0,.3);
}
